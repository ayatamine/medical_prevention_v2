                "description": "register a new doctor",
                "operationId": "register_doctor",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "full_name": {
                                        "type": "string",
                                        "example": "doctor full Name"
                                    },
                                    "id_number": {
                                        "type": "integer",
                                        "example": "46126"
                                    },
                                    "birth_date": {
                                        "type": "string",
                                        "example": "doctor birth_date"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "example": "+213664419425"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "email@email.com"
                                    },
                                    "job_title": {
                                        "type": "string",
                                        "example": "chronic disease"
                                    },
                                    "sub_specialities": {
                                        "type": "array",
                                        "items": {
                                            "type": "integer"
                                        },
                                        "example": [
                                            1,
                                            2
                                        ]
                                    },
                                    "classification_number": {
                                        "type": "string",
                                        "example": "784899554"
                                    },
                                    "insurance_number": {
                                        "type": "string",
                                        "example": "78489966554"
                                    },
                                    "medical_licence_file": {
                                        "type": "file"
                                    },
                                    "cv_file": {
                                        "type": "file"
                                    },
                                    "certification_file": {
                                        "type": "file"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Record Completed successfully",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "404": {
                        "description": "no result found with the given token or phone number, please login again",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "500": {
                        "description": "internal server error",
                        "content": {
