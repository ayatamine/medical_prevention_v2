{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ar-php/ar-php": "^1.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "cms-multi/filament-clear-cache": "^1.0",
        "darkaonline/l5-swagger": "^8.5",
        "filament/filament": "^2.17",
        "filament/spatie-laravel-translatable-plugin": "*",
        "flowframe/laravel-trend": "^0.1.5",
        "guzzlehttp/guzzle": "^7.2",
        "khaled.alshamaa/ar-php": "^6.3",
        "laravel-frontend-presets/tall": "^6.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "leandrocfe/filament-apex-charts": "^1.0",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "pusher/pusher-php-server": "^7.2",
        "stripe/stripe-php": "^10.16",
        "torann/laravel-repository": "^0.7.2",
        "twilio/sdk": "^7.4"
    },
    "require-dev": {
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^4.22",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "App\\Repositories\\": "Repositories/",
            "App\\Traits\\": "Traits/",
            "App\\Helpers\\": "Helpers/"
        },
        "files": [
            "app/Helpers/Authentication.php",
            "app/Helpers/ApiResponse.php",
            "app/Helpers/ErrorHandler.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
